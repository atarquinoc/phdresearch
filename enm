##Correlations for Ecological Niche Modelling in Tympanoctomys
#Current scenarios
current<- read.delim("C:/Users/USUARIO/OneDrive/Spatial_ecology_RVR/PAPERS/JM/Supplementary_files/currentclimate.txt")
View(current)
library(Hmisc)

vc1 <- varclus(as.matrix(current[,c(5:60)]))
plot(vc1)

